// src/integrations/TasksPluginParser.ts
var DUE_DATE_REGEX = /ðŸ“…\s*(\d{4}-\d{2}-\d{2})/;
var COMPLETION_DATE_REGEX = /âœ…\s*(\d{4}-\d{2}-\d{2})/;
function parseDueDate(rawLine) {
  const match = rawLine.match(DUE_DATE_REGEX);
  if (!match) return null;
  const d = /* @__PURE__ */ new Date(match[1] + "T00:00:00");
  return isNaN(d.getTime()) ? null : d;
}
function parseCompletionDate(rawLine) {
  const match = rawLine.match(COMPLETION_DATE_REGEX);
  if (!match) return null;
  const d = /* @__PURE__ */ new Date(match[1] + "T00:00:00");
  return isNaN(d.getTime()) ? null : d;
}
function formatDate(date) {
  const y = date.getFullYear();
  const m = String(date.getMonth() + 1).padStart(2, "0");
  const d = String(date.getDate()).padStart(2, "0");
  return `${y}-${m}-${d}`;
}
function today() {
  const d = /* @__PURE__ */ new Date();
  d.setHours(0, 0, 0, 0);
  return d;
}

// src/core/TaskParser.ts
var TASK_REGEX = /^(\s*[-*+]|\s*\d+[.)]) \[([ xX])\] (.*)$/;
function escapeRegExp(s) {
  return s.replace(/[.*+?^${}()|[\]\\]/g, "\\$&");
}
function parseFile(filePath, content) {
  const lines = content.split("\n");
  const records = [];
  for (let i = 0; i < lines.length; i++) {
    const line = lines[i];
    const match = line.match(TASK_REGEX);
    if (!match) continue;
    const checkMark = match[2];
    const rawRest = match[3];
    const isCompleted = checkMark === "x" || checkMark === "X";
    const dueDate = parseDueDate(line);
    const completedAt = isCompleted ? parseCompletionDate(line) : null;
    const tags = extractTags(rawRest);
    const inlineField = extractInlineField(rawRest);
    const text2 = stripMetadata(rawRest);
    records.push({
      id: makeId(filePath, i, text2),
      filePath,
      lineNumber: i,
      rawLine: line,
      text: text2,
      isCompleted,
      completedAt,
      dueDate,
      tags,
      inlineField,
      indentLevel: extractIndentLevel(line),
      parentId: null,
      childIds: []
    });
  }
  return buildTaskHierarchy(records);
}
function extractTags(text2) {
  const matches = text2.match(/#[\w/-]+/g) ?? [];
  return matches.map((t) => t.slice(1));
}
function extractInlineField(text2) {
  const match = text2.match(/\[[\w-]+::\s*([^\]]+)\]/);
  return match ? match[1].trim() : null;
}
function stripMetadata(text2) {
  return text2.replace(/ðŸ“…\s*\d{4}-\d{2}-\d{2}/g, "").replace(/âœ…\s*\d{4}-\d{2}-\d{2}/g, "").replace(/ðŸ”[^#[ðŸ“…âœ…]*/gu, "").replace(/[â«ðŸ”¼ðŸ”½â¬]/gu, "").replace(/#[\w/-]+/g, "").replace(/\[[\w-]+::\s*[^\]]*\]/g, "").replace(/\s{2,}/g, " ").trim();
}
function extractIndentLevel(rawLine) {
  let spaces = 0;
  for (const ch of rawLine) {
    if (ch === " ") spaces += 1;
    else if (ch === "	") spaces += 2;
    else break;
  }
  return Math.floor(spaces / 2);
}
function buildTaskHierarchy(tasks) {
  const byFile = /* @__PURE__ */ new Map();
  for (const task of tasks) {
    if (!byFile.has(task.filePath)) byFile.set(task.filePath, []);
    byFile.get(task.filePath).push(task);
  }
  const taskLookup = new Map(tasks.map((t) => [t.id, t]));
  for (const fileTasks of byFile.values()) {
    fileTasks.sort((a, b) => a.lineNumber - b.lineNumber);
    const stack2 = [];
    for (const task of fileTasks) {
      while (stack2.length > 0 && stack2[stack2.length - 1].indentLevel >= task.indentLevel) {
        stack2.pop();
      }
      if (stack2.length > 0) {
        const parentEntry = stack2[stack2.length - 1];
        task.parentId = parentEntry.id;
        const parent = taskLookup.get(parentEntry.id);
        if (parent) parent.childIds.push(task.id);
      }
      stack2.push({ indentLevel: task.indentLevel, id: task.id });
    }
  }
  return tasks;
}
function makeId(filePath, lineNumber, text2) {
  let h = 0;
  const s = `${filePath}:${lineNumber}:${text2}`;
  for (let i = 0; i < s.length; i++) {
    h = Math.imul(31, h) + s.charCodeAt(i) | 0;
  }
  return String(h >>> 0);
}
function getInlineFieldValue(rawLine, key2) {
  const re = new RegExp(`\\[${escapeRegExp(key2)}::\\s*([^\\]]+)\\]`, "i");
  const match = rawLine.match(re);
  return match ? match[1].trim() : null;
}
function setInlineFieldValue(rawLine, key2, value) {
  const escaped = escapeRegExp(key2);
  const re = new RegExp(`\\s*\\[${escaped}::\\s*[^\\]]*\\]`, "gi");
  const hasField = re.test(rawLine);
  if (value === null) {
    return rawLine.replace(re, "").trimEnd();
  }
  const token = `[${key2}:: ${value}]`;
  if (hasField) {
    return rawLine.replace(new RegExp(`\\[${escaped}::\\s*[^\\]]*\\]`, "i"), token);
  }
  return rawLine.trimEnd() + " " + token;
}
function getTagValue(rawLine, prefix) {
  const re = new RegExp(`#${escapeRegExp(prefix)}/([\\w-]+)`, "i");
  const match = rawLine.match(re);
  return match ? match[1] : null;
}
function setTagValue(rawLine, prefix, value) {
  const re = new RegExp(`\\s*#${escapeRegExp(prefix)}/[\\w-]+`, "gi");
  const cleaned = rawLine.replace(re, "").trimEnd();
  if (value === null) return cleaned;
  return cleaned + ` #${prefix}/${value}`;
}

// src/core/StorageMigrator.ts
var import_obsidian2 = require("obsidian");

// src/core/TaskWriter.ts
var import_obsidian = require("obsidian");
function findTaskLine(lines, task) {
  if (task.lineNumber < lines.length && lines[task.lineNumber] === task.rawLine) {
    return task.lineNumber;
  }
  return lines.findIndex((l) => l === task.rawLine);
}
async function moveTaskToBucket(app, task, targetBucket, settings) {
  const file = app.vault.getAbstractFileByPath(task.filePath);
  if (!(file instanceof import_obsidian.TFile)) {
    return { success: false, error: `File not found: ${task.filePath}` };
  }
  let result = { success: false, error: "Task line not found in file (stale index)" };
  try {
    await app.vault.process(file, (content) => {
      const lines = content.split("\n");
      const lineIdx = findTaskLine(lines, task);
      if (lineIdx === -1) {
        new import_obsidian.Notice(
          `GTD Tasks: Could not locate task in ${file.basename}. Re-indexing\u2026`
        );
        return content;
      }
      lines[lineIdx] = applyBucketChange(lines[lineIdx], targetBucket, settings);
      result = { success: true };
      return lines.join("\n");
    });
    return result;
  } catch (e) {
    return { success: false, error: String(e) };
  }
}
async function confirmTaskPlacement(app, task, bucketId, settings) {
  const bucket = settings.buckets.find((b) => b.id === bucketId) ?? null;
  return moveTaskToBucket(app, task, bucket, settings);
}
async function toggleTaskCompletion(app, task, settings) {
  const file = app.vault.getAbstractFileByPath(task.filePath);
  if (!(file instanceof import_obsidian.TFile)) {
    return { success: false, error: `File not found: ${task.filePath}` };
  }
  let result = { success: false, error: "Task line not found in file (stale index)" };
  try {
    await app.vault.process(file, (content) => {
      const lines = content.split("\n");
      const lineIdx = findTaskLine(lines, task);
      if (lineIdx === -1) {
        new import_obsidian.Notice(`GTD Tasks: Could not locate task in ${file.basename}.`);
        return content;
      }
      let line = lines[lineIdx];
      if (task.isCompleted) {
        line = line.replace(/\[[ xX]\]/, "[ ]");
        line = line.replace(/\s*âœ…\s*\d{4}-\d{2}-\d{2}/, "").trimEnd();
      } else {
        line = line.replace(/\[ \]/, "[x]");
        if (settings.readTasksPlugin) {
          line = line.trimEnd() + ` \u2705 ${formatDate(/* @__PURE__ */ new Date())}`;
        }
      }
      lines[lineIdx] = line;
      result = { success: true };
      return lines.join("\n");
    });
    return result;
  } catch (e) {
    return { success: false, error: String(e) };
  }
}
function applyBucketChange(rawLine, targetBucket, settings) {
  const { storageMode, tagPrefix } = settings;
  const value = targetBucket?.id ?? null;
  if (storageMode === "inline-tag") {
    return setTagValue(rawLine, tagPrefix, value);
  }
  if (storageMode === "inline-field") {
    return setInlineFieldValue(rawLine, tagPrefix, value);
  }
  return rawLine;
}

// src/core/StorageMigrator.ts
async function migrateStorageMode(app, allTasks, fromMode, toSettings) {
  const { storageMode: toMode, tagPrefix, buckets } = toSettings;
  if (fromMode === toMode) return;
  const byFile = /* @__PURE__ */ new Map();
  for (const t of allTasks) {
    if (!byFile.has(t.filePath)) byFile.set(t.filePath, []);
    byFile.get(t.filePath).push(t);
  }
  let migrated = 0;
  let failed = 0;
  for (const [filePath, tasks] of byFile) {
    const file = app.vault.getAbstractFileByPath(filePath);
    if (!(file instanceof import_obsidian2.TFile)) continue;
    let fileMigrated = 0;
    let fileFailed = 0;
    await app.vault.process(file, (content) => {
      const lines = content.split("\n");
      let changed = false;
      for (const task of tasks) {
        const bucketId = readBucketId(task, fromMode, tagPrefix);
        if (!bucketId) continue;
        if (!buckets.some((b) => b.id === bucketId)) continue;
        const lineIdx = findTaskLine(lines, task);
        if (lineIdx === -1) {
          fileFailed++;
          continue;
        }
        lines[lineIdx] = writeBucketId(lines[lineIdx], bucketId, toMode, tagPrefix, fromMode);
        changed = true;
        fileMigrated++;
      }
      return changed ? lines.join("\n") : content;
    });
    migrated += fileMigrated;
    failed += fileFailed;
  }
  new import_obsidian2.Notice(`GTD Tasks: Migration complete. ${migrated} tasks updated, ${failed} skipped.`);
}
function readBucketId(task, mode, tagPrefix) {
  switch (mode) {
    case "inline-tag":
      return getTagValue(task.rawLine, tagPrefix);
    case "inline-field":
      return getInlineFieldValue(task.rawLine, tagPrefix);
  }
}
function writeBucketId(rawLine, bucketId, toMode, tagPrefix, fromMode) {
  let line = rawLine;
  switch (fromMode) {
    case "inline-tag":
      line = setTagValue(line, tagPrefix, null);
      break;
    case "inline-field":
      line = setInlineFieldValue(line, tagPrefix, null);
      break;
  }
  switch (toMode) {
    case "inline-tag":
      return setTagValue(line, tagPrefix, bucketId);
    case "inline-field":
      return setInlineFieldValue(line, tagPrefix, bucketId);
  }
}

